package p367_3;

public class PrinterManager {
	public int counter;

	public PrinterManager(int counter) {
		this.counter = counter;
	}
	
	public static Printer getPrinter() {
		if(printer==null) {
			try {
				Thread.sleep(1);				
			}catch(InterruptedException e) {}
			
			printer=new Printer();
		}		
		return printer;
	}

}