package p367_1;

public class Printer {

	private static Printer printer=null;  // 프린터 객체 선언

	public static Printer getPrinter() {
		if(printer==null) {
			printer=new Printer();
		}		
		return printer;
	}

	private char[] str;

	public void print(String string) {
		System.out.println(str);
		
	}

}
